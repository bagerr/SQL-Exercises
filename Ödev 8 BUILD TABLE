3.--UPDATE employee
SET birthday = '2020-08-01' 
WHERE name LIKE 'E%';

UPDATE employee
SET name = 'ALİ'
WHERE id = 25;

UPDATE employee
SET name = 'ALİ'
WHERE ıd > 35;

UPDATE employee
SET birthday = '2010-01-01'
WHERE LIKE = '%e';

UPDATE employee
SET birthday = '2010-09-01'
WHERE ıd < 15;


4.--DELETE FROM Employee WHERE name='%e'

